{
  "excludes": {
    "packages": [
      "^symfony\/polyfill-",
      "^symfony\/phpunit",
      "^phpunit",
      "^mockery",
      "^behat"
    ]
  },
  "packages": {
    "mediamonks\/composer-vendor-cleaner": {
      "excludes": {
        "files": [
          "bin/clean",
          "bin/clean.php",
          "options.json"
        ]
      }
    },
    "mobiledetect\/mobiledetectlib": {
      "excludes": {
        "files": [
          "Mobile_Detect.json"
        ]
      }
    }
  }
}
